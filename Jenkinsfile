pipeline {
    agent any

    stages {
        stage('Clone') {
            steps {
                echo 'Cloning the repo...'
            }
        }
        stage('Build') {
            steps {
                echo ' Building the app...'
            }
        }
        stage('Test') {
            steps {
                echo ' Running tests...'
            }
        }
        stage('Deploy') {
            steps {
                echo ' Deploying app...'
            }
        }
    }
}
